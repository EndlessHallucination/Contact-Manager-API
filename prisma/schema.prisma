generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contact {
  id         Int      @id @default(autoincrement())
  firstName  String
  lastName   String
  street     String
  city       String
  state      String?
  postalCode String
  country    String
  email      String
  phone      String
  cell       String
  age        Int
  image      String?
  createdAt  DateTime @default(now())
}
